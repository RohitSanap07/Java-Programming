class Mango 
{
	double price ;
	String type ;
	String origin ;

	Mango()
	{}

	public mango showPrice()
	{
		System.out.println(price);
		return this;
	}

	public mango showType()
	{
		System.out.println(type);
		return this;
	}

    public mango showOrigin()
	{
		System.out.println(origin);
		return this;
	}
}
